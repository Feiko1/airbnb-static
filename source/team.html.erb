</div>

<ul class="list-inline text-center">
  <% ["feiko", "bill"].each do |member| %>
  <li>
    <h2><%= member.capitalize %></h2>
    <%= image_tag "#{member}.png", class: "avatar-large" %>
  </li>
  <% end %>
</ul>

<div class="text-center padded">
  <% link_to "contact@airbnb.com" do %>
  <i class="fa fa-envelope" style="font-size: 28px"> contact@airbnb.com</i>
  <% end %>
</div>

